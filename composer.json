{
	"name": "modette/core-ext-ui",
	"description": "UI layer for Modette applications",
	"type": "library",
	"license": "MPL-2.0",
	"keywords": [
		"controls",
		"forms",
		"presenters",
		"ui"
	],
	"homepage": "https://github.com/modette/cmf",
	"authors": [
		{
			"name": "Marek Barto≈°",
			"homepage": "https://marek-bartos.cz"
		}
	],
	"require": {
		"php": "^7.2.0",
		"contributte/application": "^0.4.0",
		"contributte/event-dispatcher-extra": "^0.6.0",
		"contributte/forms": "^0.4.0",
		"contributte/phpdoc": "^0.3.0",
		"modette/core": "^1.0.0",
		"modette/core-ext-http": "^1.0.0",
		"modette/core-ext-templates": "^1.0.0",
		"nette/component-model": "^3.0.0",
		"nette/http": "~3.0.2"
	},
	"require-dev": {
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^0.11.0",
		"phpstan/phpstan-nette": "^0.11.0",
		"phpstan/phpstan-strict-rules": "^0.11.0",
		"phpunit/phpunit": "^8.2.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Modette\\UI\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Modette\\UI\\": "tests"
		}
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
